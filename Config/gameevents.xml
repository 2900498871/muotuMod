		</action>
	</action_sequence>



	<action_sequence name="game_流放"><!--传送-将目标传送到指定的位置或偏移。-->
		<requirement class="InVehicle"><!--车内-->
			<property name="invert" value="true" /><!--倒转-->
		</requirement>
		<action class="Teleport"><!--传送-->
			<property name="target_position" value="0, 0, 20" /><!--目标_位置-->
			<property name="offset_type" value="Relative" /><!--偏移类型  相对的-->
		</action>
	</action_sequence>
	
	
	
	
	
	
	
